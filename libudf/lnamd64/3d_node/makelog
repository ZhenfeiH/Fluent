# Generating udf_names.c because of makefile src/udf.c src/mymath.c src/reports.c
make libudf.so "CFLAGS=-D_lnamd64 -D_GNU_SOURCE -fpic -shared -ansi -Wall -O -DPTR_RESTRICT=	" "LDFLAGS=-shared -lm"
# Compiling udf_names.o because of udf_names.c
cc -D_lnamd64 -D_GNU_SOURCE -fpic -shared -ansi -Wall -O -DPTR_RESTRICT=	 -I/usr/local/apps64/ansys_inc/v145/fluent/fluent14.5.0/`expr "\`pwd\`" : '.*/\(.*\)/[23].*'`/`basename "\`pwd\`"` -I/usr/local/apps64/ansys_inc/v145/fluent/fluent14.5.0/src -I/usr/local/apps64/ansys_inc/v145/fluent/fluent14.5.0/cortex/src -I/usr/local/apps64/ansys_inc/v145/fluent/fluent14.5.0/client/src -I/usr/local/apps64/ansys_inc/v145/fluent/fluent14.5.0/tgrid/src -I/usr/local/apps64/ansys_inc/v145/fluent/fluent14.5.0/multiport/src -I/usr/local/apps64/ansys_inc/v145/fluent/fluent14.5.0/multiport/mpi_wrapper/include -I/usr/local/apps64/ansys_inc/v145/fluent/fluent14.5.0/multiport/mpi_wrapper/src -I. -c udf_names.c -o udf_names.o
# Linking libudf.so because of makefile user.udf udf_names.c udf_names.o src/udf.o src/mymath.o src/reports.o
ld -shared -lm udf_names.o src/udf.o src/mymath.o src/reports.o  -o libudf.so
